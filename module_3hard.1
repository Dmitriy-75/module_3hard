

def count_(m):
    for i in m:

        if isinstance(i, tuple):
            m=i
            t = count_(m)
            print(m,type(m),t)

        if isinstance(i, int):
            int_ = i
            print(i, type(i), int_)


        if isinstance(i, str):
           print(i , type(i),len(i))



        if isinstance(i, list):
           print(i,type(i),sum(i))


        if isinstance(i, dict):
           keys_len = len("".join(list(i)))
           value_sum=sum(i.values())
           dict_sum =keys_len+value_sum
           print(i,type(i),dict_sum)



